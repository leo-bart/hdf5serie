prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
bindir=@bindir@
datadir=@datadir@
PACKAGE_TARNAME=@PACKAGE_TARNAME@
docdir=@docdir@

DOXYGENTAGFILES=${docdir}/@PACKAGE@.tag

hdf5_prefix=@hdf5_prefix@

Name: hdf5serie
Description: A c++ wrapper for HDF5 to write data series
Requires: fmatvec
Version: @hdf5serie_VERSION@
Libs: -L${libdir} -lhdf5serie @HDF5_INCLUDE_DIRS@ @LIBS@ @Boost_SYSTEM_LIBRARY@ @Boost_FILESYSTEM_LIBRARY@ 
Cflags: -I${includedir} @HDF5CPPFLAGS@
